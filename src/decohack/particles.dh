thing 250 "Ember"
{
	health 1000
	speed 0
	radius 0
	height 0
	damage 0
	reactiontime 8
	painchance 0
	mass 100
	+missile
	+noblockmap
	+nogravity
	+translucent
	states
	{
		spawn:
			TNT1 A 1
			goto pick
