			goto xdeath
		type3:
			SP98 ONML 2 bright
			SP98 K 16 bright a_removeflags(translucent)
			SP98 LM 14 bright
			SP98 M 12 bright a_addflags(translucent)
			SP98 NO 12 bright
			goto xdeath
		death:
			TNT1 A 1
			stop
		xdeath:
			TNT1 A 1
			stop
	}
}

