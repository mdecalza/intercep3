			TNT1 A 0 {
			a_randomjump(type2,85)
			a_randomjump(type3,127)
			}
			goto type1
		type1:
			SP98 EDCB 2 bright
			SP98 A 10 bright a_removeflags(translucent)
			SP98 BC 8 bright
			SP98 C 6 bright a_addflags(translucent)
			SP98 DE 6 bright
			goto xdeath
		type2:
			SP98 JIHG 2 bright
			SP98 F 13 bright a_removeflags(translucent)
